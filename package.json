{
  "name": "questdb-sql-parser",
  "version": "0.1.0",
  "description": "SQL parser for QuestDB syntax using Chevrotain",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "packageManager": "yarn@4.9.1",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist coverage"
  },
  "keywords": [
    "questdb",
    "sql",
    "parser",
    "chevrotain",
    "ast"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "chevrotain": "^11.1.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
